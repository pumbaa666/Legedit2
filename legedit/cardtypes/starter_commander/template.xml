<?xml version="1.0" encoding="utf-8"?>
<xml>

	<template name="starter_commander" displayname="Starter Commander" deck="starter_deck"
		defaultsindeck="true" />

	<cardsize cardwidth="750" cardheight="1050" dpi="300" />

	<bgimage name="Image" fullsize="false" x="0" y="0" maxwidth="750"
		maxheight="1050" allowchange="true" zoomable="true" />

	<image name="back_cost" templatefile="true" path="back_cost.png"
		fullsize="true" x="0" y="0" maxwidth="750" maxheight="1050"
		allowchange="false" zoomable="false" />

	<styles>

		<style name="Villains">

			<cardname name="Card Name" defaultvalue="Card Name"
				allowchange="true" includesubname="true" highlight="banner"
				highlightcolour="#111111" uppercase="true" alignment="center" x="375"
				y="40" colour="#e9dc98" textsize="48" subnametext="HERO" subnameeditable="true"
				subnamesize="34" subnamegap="15" blurradius="15" bluedouble="true"
				blurexpand="4" />

			<icon name="Team" defaultvalue="none" allowchange="true"
				icontype="team" x="50" y="50" maxwidth="60" maxheight="60"
				drawunderlay="true" blurradius="10" bluedouble="false" blurexpand="2"
				blurcolour="#000000" />

			<icon name="Power" defaultvalue="none" allowchange="true"
				icontype="power" x="50" y="120" maxwidth="60" maxheight="60"
				drawunderlay="true" blurradius="10" bluedouble="false" blurexpand="2"
				blurcolour="#000000" />

		</style>

		<style name="Base Set">

			<cardname name="Card Name" defaultvalue="Card Name"
				allowchange="true" includesubname="true" highlight="blur"
				highlightcolour="#111111" uppercase="true" alignment="center" x="375"
				y="40" colour="#e9dc98" textsize="48" subnametext="HERO" subnameeditable="true"
				subnamesize="34" subnamegap="15" blurradius="15"
				bluedouble="true" blurexpand="4" />

			<icon name="Team" defaultvalue="none" allowchange="true"
				icontype="team" x="50" y="50" maxwidth="60" maxheight="60"
				drawunderlay="true" blurradius="10" bluedouble="false" blurexpand="2"
				blurcolour="#000000" />

			<icon name="Power" defaultvalue="none" allowchange="true"
				icontype="power" x="50" y="120" maxwidth="60" maxheight="60"
				drawunderlay="true" blurradius="10" bluedouble="false" blurexpand="2"
				blurcolour="#000000" />

		</style>

	</styles>

	<elementgroup name="Attack">

		<icon name="Attack Icon" defaultvalue="attack" allowchange="false"
			optional="true" x="82" y="917" maxwidth="130" maxheight="130"
			drawunderlay="true" blurradius="15" bluedouble="false" blurexpand="0"
			blurcolour="#000000" />

		<text name="Attack Value" linkedelement="Attack" alignment="center"
			allowchange="true" defaultvalue="0" x="82" y="917" colour="#FFFFFF"
			textsize="110" uppercase="true" drawunderlay="true" blurradius="2"
			bluedouble="false" blurexpand="3" blurcolour="#000000" />

	</elementgroup>

<text
 name="Cost Value"
 alignment="center"
 allowchange="true"
 defaultvalue="X"
 x="635"
 y="950"
 colour="#FFFFFF"
 textsize="120"
 uppercase="true"
 drawunderlay="true"
 blurradius="2"
 bluedouble="false"
 blurexpand="3"
 blurcolour="#000000"
 />
 
	<property name="Number in Deck" property="numberindeck"
		defaultvalue="4" />

</xml>

