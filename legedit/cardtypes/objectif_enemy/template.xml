<?xml version="1.0" encoding="utf-8"?>
<xml>

	<template name="objectif_enemy" displayname="Ennemi" deck="objectif"
		defaultsindeck="true" />

	<cardsize cardwidth="750" cardheight="1050" dpi="300" />

	<bgimage name="Image" fullsize="false" x="0" y="0" maxwidth="750"
		maxheight="1050" allowchange="true" zoomable="true" />

	<image name="back_underlay" templatefile="true" path="back_underlay.png"
		fullsize="true" x="0" y="0" maxwidth="750" maxheight="1050"
		allowchange="false" zoomable="false" />

	<styles>

		<style name="Villains">

			<cardname name="Card Name" defaultvalue="Card Name"
				allowchange="true" includesubname="true" highlight="banner"
				highlightcolour="#111111" uppercase="true" alignment="center" x="375"
				y="40" colour="#e9dc98" textsize="48" subnametext="%DECKNAME%"
				subnameeditable="true" subnamesize="34" subnamegap="15" blurradius="15"
				bluedouble="true" blurexpand="4" />

			<icon name="Team" defaultvalue="none" allowchange="true"
				icontype="team" x="50" y="50" maxwidth="60" maxheight="60"
				drawunderlay="true" blurradius="10" bluedouble="false" blurexpand="2"
				blurcolour="#000000" />

		</style>

		<style name="Base Set">

			<cardname name="Card Name" defaultvalue="Card Name"
				allowchange="true" includesubname="true" highlight="blur"
				highlightcolour="#111111" uppercase="true" alignment="center" x="375"
				y="40" colour="#e9dc98" textsize="48" subnametext="%DECKNAME%"
				subnameeditable="true" subnamesize="34" subnamegap="15" blurradius="15"
				bluedouble="true" blurexpand="4" />

			<icon name="Team" defaultvalue="none" allowchange="true"
				icontype="team" x="50" y="50" maxwidth="60" maxheight="60"
				drawunderlay="true" blurradius="10" bluedouble="false" blurexpand="2"
				blurcolour="#000000" />

		</style>

	</styles>

	<elementgroup name="Victory">

		<icon name="Victory Icon" defaultvalue="victory" allowchange="false"
			optional="true" x="662" y="696" maxwidth="60" maxheight="60"
			drawunderlay="false" blurradius="1" bluedouble="false" blurexpand="0"
			blurcolour="#000000" />

		<text name="Victory Value" linkedelement="Victory" alignment="center"
			allowchange="true" defaultvalue="0" x="662" y="696" colour="#FFFFFF"
			textsize="40" uppercase="true" drawunderlay="true" blurradius="2"
			bluedouble="false" blurexpand="3" blurcolour="#000000" />

	</elementgroup>


	<icon name="Attack" defaultvalue="attack" allowchange="false"
		optional="true" x="645" y="950" maxwidth="160" maxheight="160"
		drawunderlay="true" blurradius="3" bluedouble="false" blurexpand="0"
		blurcolour="#000000" />

	<text name="Attack Value" linkedelement="Attack" alignment="center"
		allowchange="true" defaultvalue="0" x="645" y="950" colour="#FFFFFF"
		textsize="110" uppercase="true" drawunderlay="true" blurradius="2"
		bluedouble="false" blurexpand="3" blurcolour="#000000" />

	<textarea name="Card Text" alignmenthorizontal="left"
		alignmentvertical="top" allowchange="true" defaultvalue="Card Text."
		colour="#000000" textsize="34" rectxarray="90,660,660,555,555,90"
		rectyarray="730,730,880,880,965,1000" debug="false" />

	<property name="Number in Deck" property="numberindeck"
		defaultvalue="3" />

</xml>

